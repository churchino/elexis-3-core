               id="ch.elexis.eigenleistung.edit.selected"
               name="Selected Eigenleistung"
               optional="true"
               typeId="ch.elexis.PersistentObjectType">
         </commandParameter>
      </command>
      <commandParameterType
            converter="ch.elexis.core.ui.commands.PersistentObjectConverter"
            id="ch.elexis.PersistentObjectType"
            type="ch.elexis.core.data.PersistentObject">
