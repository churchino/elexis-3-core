      <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="ch.elexis.toolbar1">
            <command
                  commandId="org.eclipse.ui.views.showView"
                  icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/new_wiz.gif"
                  label="Views anzeigen"
                  style="push"
                  tooltip="%command.showViews">
               <visibleWhen
                     checkEnabled="false">
                  <test
                        args="AC_SHOWVIEW"
                        property="ch.elexis.commands.propertyTester.ACE">
                  </test>
               </visibleWhen>
            </command>
         </toolbar>
         <toolbar
               id="ch.elexis.coolbar.MandantSelection">
            <dynamic
                  class="ch.elexis.core.ui.coolbar.MandantSelectionContributionItem"
                  id="ch.elexis.core.ui.coolbar.MandantSelectionContributionItem">
            </dynamic>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:ch.elexis.LeistungenView">
         <command
               commandId="org.eclipse.ui.file.properties"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/system-search-3.png"
               label="%properties"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:ch.elexis.PatListView">
         <command
               commandId="org.eclipse.ui.file.properties"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/system-search-3.png"
               label="%properties"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:ch.elexis.codedetailview">
         <command
               commandId="org.eclipse.ui.file.properties"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/system-search-3.png"
               label="%properties"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:help">
         <command
               commandId="org.eclipse.ui.window.showKeyAssist"
               label="Key Assist..."
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:ch.elexis.BehandlungenVerrechnenView">
         <command
               commandId="ch.elexis.core.command.linkViews"
               icon="icon://IMG_SYNC"
               label="Patientenauswahl verlinken"
               style="toggle">
         </command>
      </menuContribution>
