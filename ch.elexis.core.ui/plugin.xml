      <extension
            point="org.eclipse.ui.services">
         <sourceProvider
               provider="ch.elexis.core.ui.commands.sourceprovider.PatientSelectionStatus">
            <variable
                  name="ch.elexis.core.ui.commands.sourceprovider.patientSelectionActive"
                  priorityLevel="workbench">
