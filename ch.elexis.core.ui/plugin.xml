               id="ch.elexis.eigenartikel.edit.selected"
               name="Selected Eigenartikel"
               optional="true"
               typeId="ch.elexis.PersistentObjectType">
         </commandParameter>
      </command>
      <command
            defaultHandler="ch.elexis.core.ui.commands.EditEigenleistungUi"
            id="ch.elexis.eigenleistung.edit"
            name="Eigenleistung Ändern">
