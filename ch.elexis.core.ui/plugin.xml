   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="ch.elexis.core.ui.commands.kategorie"
            id="ch.elexis.core.ui.commands.login"
            name="%elexis.loginCommand"/>
      <category
            id="ch.elexis.core.ui.commands.kategorie"
            name="%category.name.0"/>
      <command
            categoryId="ch.elexis.core.ui.commands.kategorie"
            id="ch.elexis.core.ui.commands.import"
            name="%elexis.importCommand"/>
      <command
            categoryId="ch.elexis.core.ui.commands.kategorie"
            description="%command.description.0"
            id="ch.elexis.core.ui.commands.connectWizard"
            name="%elexis.connectCommand"/>
      <command
            description="%command.description.1"
            id="ch.elexis.core.ui.commands.home"
            name="%command.name.0"/>
      <command
            categoryId="ch.elexis.core.ui.commands.bills"
            defaultHandler="ch.elexis.core.ui.commands.ErstelleRnnCommand"
            id="bill.create"
            name="%command.name.1">
         <commandParameter
               id="ch.elexis.RechnungErstellen.parameter"
               name="%commandParameter.name.0"
               optional="true"
               typeId="ch.elexis.TreeType">
         </commandParameter>
      </command>
      <category
            description="%category.description.0"
            id="ch.elexis.core.ui.commands.bills"
            name="%category.name.1">
      </category>
      <command
            categoryId="ch.elexis.core.ui.commands.bills"
            defaultHandler="ch.elexis.core.ui.commands.MahnlaufCommand"
            description="%command.description.2"
            id="bill.reminder"
            name="%command.name.2">
      </command>
      <commandParameterType
            converter="ch.elexis.core.ui.commands.TreeToStringConverter"
            id="ch.elexis.TreeType"
            type="ch.rgw.tools.Tree">
      </commandParameterType>
      <command
            categoryId="ch.elexis.core.ui.commands.kategorie"
            defaultHandler="ch.elexis.core.ui.commands.ExportiereBloeckeCommand"
            description="%command.description.3"
            id="serviceblocks.export"
            name="%command.name.3">
      </command>
