               id="ch.elexis.RezeptBlatt">
         </OutputLogDescriptor>
      </extension>
      <extension
            point="org.eclipse.ui.propertyPages">
         <page
               class="ch.elexis.core.ui.propertypage.PatientPropertyPage"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/personal.png"
               id="at.medevit.elexis.properties.propertyPage.PatientPropertyPage"
               name="%details">
            <enabledWhen>
               <instanceof
                     value="ch.elexis.data.Patient">
               </instanceof>
            </enabledWhen>
         </page>
         <page
               class="ch.elexis.core.ui.propertypage.PatientAddressPage"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/address-book-new-2.png"
               id="at.medevit.elexis.properties.propertyPage.PatientAddressPage"
               name="%address">
            <enabledWhen>
               <instanceof
                     value="ch.elexis.data.Patient">
               </instanceof>
            </enabledWhen>
         </page>
         <page
               class="ch.elexis.core.ui.propertypage.XIDPropertyPage"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/emblem-symbolic-link-2.png"
               id="at.medevit.elexis.properties.propertyPage.XIDPropertyPage"
               name="XID">
            <enabledWhen>
               <instanceof
                     value="ch.elexis.data.PersistentObject">
               </instanceof>
            </enabledWhen>
         </page>
         <page
               class="ch.elexis.core.ui.propertypage.ExtInfoPropertyPage"
               icon="platform:/plugin/ch.elexis.core.ui.icons/icons/16x16/emblem-symbolic-link-2.png"
               id="at.medevit.elexis.properties.propertyPage.ExtInfoPropertyPage"
               name="ExtInfo">
         </page>
      </extension>
      <extension
            point="org.eclipse.ui.bindings">
         <key
               commandId="org.eclipse.ui.window.showKeyAssist"
               schemeId="ch.elexis.ui.keybindingScheme"
               sequence="M1+SHIFT+L">
         </key>
         <scheme
               description="KeyBindings that use this scheme will be displayed by the KeyAssistant in Elexis"
               id="ch.elexis.ui.keybindingScheme"
               name="Elexis Keybinding Scheme">
         </scheme>
